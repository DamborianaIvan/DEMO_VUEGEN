	<script>
		function getLang() {
			if (navigator.language != undefined) 
				return navigator.language;
			else if(navigator.browserLanguage != undefined)
				return  navigator.browserLanguage;
			else 
					return "";
		}
							 
	function appendSplashImage() { 
		 var locales =  ["en_US","es_ES"]; // Locales
		 var browserLocale = getLang().replace("-","_"); 
		 var defaultLocale = "es_ES"; // default locale
		 var splashImage = "loader_loop.gif";
		 var ss=document.getElementById("splashScreen"); 
		 if(ss) { 
			if(browserLocale && browserLocale !="" && locales.indexOf(browserLocale) >= 0 ){
				ss.style.background = " #ffffff url('desktopweb/images/"+browserLocale+"/"+splashImage+"') no-repeat fixed center center"; 
			} else if(defaultLocale != "") {
				ss.style.background = " #ffffff url('desktopweb/images/"+defaultLocale+"/"+splashImage+"') no-repeat fixed center center";
			} else {
				ss.style.background = " #ffffff url('desktopweb/images/"+splashImage+"') no-repeat fixed center center";
			}
		 }	
	}
	setTimeout(function(){appendSplashImage()},100); 
	</script>
	 

